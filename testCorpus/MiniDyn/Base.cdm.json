{
    "$schema": "../schema.cdm.json",
    "schemaVersion": "0.7.0",
    "imports": [
        {
            "uri": "/foundations.cdm.json"
        },
        {
            "uri": "User.cdm.json"
        }
    ],
    "definitions": [
        {
            "traitName": "perfStandard",
            "extendsTrait": "is",
            "hasParameters": [
                {
                    "name": "P1",
                    "dataType": "string",
                    "defaultValue": "P1Def"
                }
            ]
        },
        {
            "entityName": "Base",
            "extendsEntity": "CdmObject",
            "description": "This is the base class",
            "displayName": "EntityDisplay",
            "exhibitsTraits": [
                {
                    "traitReference": "perfStandard",
                    "arguments": [
                        {
                            "name": "P1",
                            "value": "P1FromBase"
                        }
                    ]
                }
            ],
            "hasAttributes": [
                {
                    "attributeGroupReference": {
                        "attributeGroupName": "base",
                        "exhibitsTraits": [
                            {
                                "traitReference": "is.CDM.attributeGroup",
                                "arguments": [
                                    {
                                        "entityReference": {
                                            "entityShape": "attributeGroupSet",
                                            "constantValues": [
                                                [
                                                    "fromTheBase"
                                                ]
                                            ]
                                        }
                                    }
                                ]
                            }
                        ],
                        "members": [
                            {
                                "name": "createdOn",
                                "relationship": "hasA",
                                "dataType": "dateTime",
                                "description": "This is example description text",
                                "displayName": "AttributeDisplay"
                            },
                            {
                                "name": "modifiedOn",
                                "relationship": "hasA",
                                "dataType": "dateTime",
                                "description": "This is example description text",
                                "displayName": "AttributeDisplay"
                            },
                            {
                                "name": "owner",
                                "relationship": {
                                    "relationshipReference": "hasFlexibleRelationshipWithEntity",
                                    "appliedTraits": [
                                        {
                                            "traitReference": "does.referenceEntityVia",
                                            "arguments": [
                                                {
                                                    "name": "foreignKeyAttribute",
                                                    "value": {
                                                        "name": "ownerId",
                                                        "relationship": "hasA",
                                                        "dataType": "integer",
                                                        "description": "This is example description text",
                                                        "displayName": "AttributeDisplay"
                                                    }
                                                }
                                            ]
                                        }
                                    ]
                                },
                                "entity": {
                                    "entityReference": "User",
                                    "appliedTraits": [
                                        {
                                            "traitReference": "is.identifiedBy",
                                            "arguments": [
                                                "User/(resolvedAttributes)/userId"
                                            ]
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                }
            ]
        }
    ]
}