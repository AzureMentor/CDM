                            }
                        ]
                    }
                },
                {
                    "name": "useLinked2",
                    "relationship": {
                        "relationshipReference": "referencesA",
                        "appliedTraits": [
                            {
                                "traitReference": "referencesA/exhibitsTraits/does.referenceEntity",
                                "arguments": [
                                    {
                                        "name": "addedAttribute",
                                        "value": {
