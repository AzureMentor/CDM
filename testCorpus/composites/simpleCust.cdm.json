{
    "$schema": "../schema.cdm.json",
    "schemaVersion": "0.7.0",
    "imports": [
        {
            "corpusPath": "/foundations.cdm.json"
        }
    ],
    "definitions": [
        {
            "traitName": "is.format.integer"
        },
        {
            "traitName": "is.format.string"
        },
        {
            "traitName": "is.constrainted"
        },
        {
            "traitName": "means.personName"
        },
        {
            "traitName": "is.constrained.length",
            "extendsTrait": "is.constrainted",
            "hasParameters": [
                {
                    "name": "maxLength",
                    "dataType": "int",
                    "defaultValue": "255"
                }
            ]
        },
        {
            "dataTypeName": "int",
            "exhibitsTraits": [
                "is.format.integer"
            ]
        },
        {
            "dataTypeName": "str",
            "exhibitsTraits": [
                "is.format.string"
            ]
        },
        {
            "dataTypeName": "name",
            "extendsDataType": "str",
            "exhibitsTraits": [
                "means.personName",
                "is.constrained.length"
            ]
        },
        {
            "entityName": "address",
            "hasAttributes": [
                {
                    "name": "cid",
                    "dataType": "int"
                },
                {
                    "name": "street",
                    "dataType": "str"
                },
                {
                    "name": "cityName",
                    "dataType": "name"
                }
            ]
        },
        {
            "entityName": "simpleCust",
            "hasAttributes": [
                {
                    "name": "id",
                    "dataType": "int"
                },
                {
                    "name": "fullName",
                    "dataType": "name",
                    "appliedTraits": [
                        {
                            "traitReference": "is.constrained.length",
                            "arguments": [
                                {
                                    "name": "maxLength",
                                    "value": "130"
                                }
                            ]
                        }
                    ]
                },
                {
                    "name": "homeAddress",
                    "entity": {
                        "entityReference": "address",
                        "appliedTraits": [
                            {
                                "traitReference": "is.identifiedBy",
                                "arguments": [
                                    "address/(resolvedAttributes)/cid"
                                ]
                            }
                        ]
                    },
                    "relationship": "hasFlexibleRelationshipWithEntity"
                }
            ]
        }
    ]
}